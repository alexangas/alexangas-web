{
  "name": "alexangas-web",
  "private": true,
  "description": "Based on the personal blog starter for a blog powered by Netlify CMS",
  "version": "1.0.0",
  "author": "Alex Angas",
  "dependencies": {
    "@mdx-js/mdx": "~1.6.1",
    "@mdx-js/react": "~1.6.1",
    "bulma": "^0.8.2",
    "gatsby": "~2.21.11",
    "gatsby-image": "~2.4.0",
    "gatsby-plugin-feed-mdx": "~1.0.1",
    "gatsby-plugin-google-analytics": "~2.3.0",
    "gatsby-plugin-manifest": "~2.4.1",
    "gatsby-plugin-mdx": "~1.2.5",
    "gatsby-plugin-meta-redirect": "~1.1.1",
    "gatsby-plugin-netlify-cms": "~4.3.0",
    "gatsby-plugin-offline": "~3.2.0",
    "gatsby-plugin-react-helmet": "~3.3.0",
    "gatsby-plugin-sass": "~2.3.1",
    "gatsby-plugin-sharp": "~2.6.0",
    "gatsby-plugin-sitemap": "~2.4.1",
    "gatsby-remark-copy-linked-files": "~2.3.1",
    "gatsby-remark-images": "~3.3.0",
    "gatsby-remark-responsive-iframe": "~2.4.1",
    "gatsby-remark-smartypants": "~2.3.0",
    "gatsby-remark-vscode": "~2.0.3",
    "gatsby-source-filesystem": "~2.3.0",
    "gatsby-transformer-sharp": "~2.5.0",
    "netlify-cms-app": "~2.12.11",
    "node-sass": "~4.14.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-helmet": "~6.0.0",
    "react-icons": "^3.10.0",
    "remark-grid-tables": "~2.1.1",
    "typeface-lato": "~0.0.75"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "axe-core": "^3.5.3",
    "babel-jest": "^25.5.1",
    "babel-preset-gatsby": "^0.4.0",
    "cypress": "^4.5.0",
    "cypress-axe": "^0.8.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.0"
  },
  "homepage": "https://www.alexangas.com",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexangas/alexangas-web.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "up": "docker-compose up --build",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "stop": "docker-compose stop",
    "format": "prettier --write **/*.{js,jsx}",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  }
}
