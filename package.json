{
  "name": "alexangas-web",
  "private": true,
  "description": "Based on the personal blog starter for a blog powered by Netlify CMS",
  "version": "1.0.0",
  "author": "Alex Angas",
  "sideEffects": false,
  "dependencies": {
    "@mdx-js/mdx": "~1.6.4",
    "@mdx-js/react": "~1.6.4",
    "axios": "~0.19.2",
    "bulma": "~0.8.2",
    "clsx": "~1.1.0",
    "dayjs": "~1.8.27",
    "gatsby": "~2.22.11",
    "gatsby-image": "~2.4.5",
    "gatsby-plugin-feed-mdx": "~1.0.1",
    "gatsby-plugin-google-analytics": "~2.3.2",
    "gatsby-plugin-manifest": "~2.4.9",
    "gatsby-plugin-mdx": "~1.2.12",
    "gatsby-plugin-meta-redirect": "~1.1.1",
    "gatsby-plugin-netlify-cms": "~4.3.3",
    "gatsby-plugin-react-helmet": "~3.3.2",
    "gatsby-plugin-sass": "~2.3.2",
    "gatsby-plugin-sharp": "~2.6.9",
    "gatsby-plugin-sitemap": "~2.4.3",
    "gatsby-remark-copy-linked-files": "~2.3.3",
    "gatsby-remark-images": "~3.3.8",
    "gatsby-remark-smartypants": "~2.3.2",
    "gatsby-remark-vscode": "~2.1.1",
    "gatsby-source-filesystem": "~2.3.8",
    "gatsby-transformer-sharp": "~2.5.3",
    "lodash": "~4.17.15",
    "netlify-cms-app": "~2.12.13",
    "node-sass": "~4.14.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-helmet": "~6.0.0",
    "react-icons": "~3.10.0",
    "remark-grid-tables": "~2.1.1",
    "typeface-lato": "~0.0.75",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "axe-core": "^3.5.3",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.4.7",
    "cross-env": "^7.0.2",
    "cypress": "^4.7.0",
    "cypress-audit": "0.2.1",
    "cypress-axe": "^0.8.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.0"
  },
  "homepage": "https://www.alexangas.com",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexangas/alexangas-web.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "up": "docker-compose up --build",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "down": "docker-compose down",
    "format": "prettier --write **/*.{js,jsx,ts,tsx}",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
    "cy:open": "cypress open",
    "cy:open:prod": "cross-env CYPRESS_BASE_URL=http://localhost:9000 cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  }
}
