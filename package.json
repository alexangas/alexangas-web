    "netlify-cms-app": "^2.12.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-lato": "^0.0.75",
    "typography": "^0.16.19",
    "typography-plugin-code": "^0.16.19",
    "typography-theme-noriega": "^0.16.19"
  },
  "devDependencies": {
    "prettier": "^2.0.4"
  },
  "homepage": "https://www.alexangas.com",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexangas/alexangas-web.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "up": "docker-compose up --build",
    "stop": "docker-compose stop",
    "docker-build": "docker build -t alexangas/alexangas-web .",
    "docker-run": "docker run --rm -p 8000:8000 alexangas/alexangas-web:latest"
  }
