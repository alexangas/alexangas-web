{
  "name": "alexangas-web",
  "private": true,
  "description": "The personal web site for Alex Angas.",
  "version": "1.0.0",
  "author": "Alex Angas",
  "sideEffects": false,
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "axios": "~0.21.1",
    "bulma": "~0.9.2",
    "case-anything": "~1.1.3",
    "clsx": "~1.1.1",
    "dayjs": "~1.10.7",
    "fontsource-lato": "^4.0.0",
    "gatsby": "~2.31.1",
    "gatsby-image": "~2.10.0",
    "gatsby-plugin-feed": "~2.12.0",
    "gatsby-plugin-google-analytics": "~2.10.0",
    "gatsby-plugin-manifest": "~2.11.0",
    "gatsby-plugin-meta-redirect": "~1.1.1",
    "gatsby-plugin-react-helmet": "~3.9.0",
    "gatsby-plugin-sass": "~3.1.0",
    "gatsby-plugin-sharp": "~2.13.1",
    "gatsby-plugin-sitemap": "~2.11.0",
    "gatsby-remark-copy-linked-files": "~2.9.0",
    "gatsby-remark-images": "~3.10.0",
    "gatsby-remark-smartypants": "~2.9.0",
    "gatsby-remark-vscode": "~3.2.0",
    "gatsby-source-filesystem": "~2.10.0",
    "gatsby-transformer-remark": "~2.15.0",
    "gatsby-transformer-sharp": "~2.11.0",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-helmet": "~6.1.0",
    "sass": "^1.43.3",
    "typescript": "~4.2.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axe-core": "^4.1.1",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.11.0",
    "cypress": "^6.3.0",
    "cypress-audit": "0.3.0",
    "cypress-axe": "^0.12.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.7"
  },
  "homepage": "https://www.alexangas.com",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexangas/alexangas-web.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "up": "docker-compose up --build",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "down": "docker-compose down",
    "format": "prettier --write **/*.{js,jsx,ts,tsx}",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=text",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=text",
    "test:updatesnapshot": "jest --updateSnapshot",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  }
}
