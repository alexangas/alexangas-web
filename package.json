{
  "name": "alexangas-web",
  "private": true,
  "description": "The personal web site for Alex Angas.",
  "version": "1.0.0",
  "author": "Alex Angas",
  "sideEffects": false,
  "dependencies": {
    "axios": "~0.19.2",
    "bulma": "~0.9.0",
    "case-anything": "~1.1.1",
    "clsx": "~1.1.1",
    "dayjs": "~1.8.28",
    "gatsby": "~2.23.11",
    "gatsby-image": "~2.4.9",
    "gatsby-plugin-feed": "~2.5.7",
    "gatsby-plugin-google-analytics": "~2.3.6",
    "gatsby-plugin-manifest": "~2.4.14",
    "gatsby-plugin-meta-redirect": "~1.1.1",
    "gatsby-plugin-react-helmet": "~3.3.6",
    "gatsby-plugin-sass": "~2.3.6",
    "gatsby-plugin-sharp": "~2.6.14",
    "gatsby-plugin-sitemap": "~2.4.7",
    "gatsby-remark-copy-linked-files": "~2.3.7",
    "gatsby-remark-images": "~3.3.13",
    "gatsby-remark-smartypants": "~2.3.6",
    "gatsby-remark-vscode": "~2.1.2",
    "gatsby-source-filesystem": "~2.3.14",
    "gatsby-transformer-remark": "~2.8.19",
    "gatsby-transformer-sharp": "~2.5.7",
    "node-sass": "~4.14.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-helmet": "~6.1.0",
    "react-icons": "~3.10.0",
    "typeface-lato": "~0.0.75",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "axe-core": "^3.5.5",
    "babel-jest": "^26.1.0",
    "babel-preset-gatsby": "^0.4.12",
    "cypress": "^4.9.0",
    "cypress-audit": "0.2.1",
    "cypress-axe": "^0.8.1",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.0"
  },
  "homepage": "https://www.alexangas.com",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexangas/alexangas-web.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "up": "docker-compose up --build",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "down": "docker-compose down",
    "format": "prettier --write **/*.{js,jsx,ts,tsx}",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=text",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=text",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  }
}
